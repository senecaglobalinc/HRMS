{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft*": "Warning"
    }
  },
  "Debug": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft*": "Warning"
    }
  },
  "LogLevel": {
    "Default": "Information",
    "Microsoft": "Warning",
    "Microsoft.Hosting.Lifetime": "Information",
    "Microsoft*": "Warning"
  },
  "AllowedHosts": "*",
  "SeriLog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }

    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Logs\\RedisDataCacheService\\Log.txt",
          "outputTemplate": "{Timestamp:G} {Message} {NewLine:1} {Exception:1}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Logs\\RedisDataCacheService\\Log.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter,Serilog"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "OrgCoreDB": "Host=192.168.11.5;Port=5432;Username=hrmsorgdevdb;Password=hrmsorgdevpwd;Database=HRMSOrgCoreDB_Dev;",
    "EmployeeDB": "Host=192.168.11.5;Port=5432;Username=hrmsempdevdb;Password=hrmsempdevpwd;Database=HRMSEmployeeDB_Dev;",
    "ProjectDB": "Host=192.168.11.5;Port=5432;Username=hrmsprojdevdb;Password=hrmsprojdevpwd;Database=HRMSProjectDB_Dev;"
  },
  "redis": {
    "connection": "127.0.0.1:6379"
  }
